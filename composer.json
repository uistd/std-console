{
  "name": "ffan/std-console",
  "description": "FFan STD Console",
  "keywords": [
    "ffan",
    "console"
  ],
  "type": "composer",
  "authors": [
    {
      "name": "huangshunzhao",
      "email": "huangshunzhao@wanda.cn",
      "role": "Founder"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "ffan/std-common": "2.*",
    "ffan/std-logger" : "1.*",
    "ffan/std-event" : "2.*"
  },
  "autoload": {
    "psr-4": {
      "FFan\\Std\\Console\\" : "src"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "minimum-stability": "dev",
  "archive": {
    "exclude": [
      "test",
      ".gitignore"
    ]
  },
  "config": {
    "secure-http": false
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://composer.intra.ffan.com/repo/private/"
    },
    {
      "type": "composer",
      "url": "http://composer.intra.ffan.com/repo/packagist/"
    },
    {
      "packagist": false
    }
  ]
}